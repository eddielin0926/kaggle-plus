{
  "manifest_version": 3,
  "name": "Kaggle Plus",
  "version": "0.1.0",
  "description": "Kaggle Plus is a browser extension that visualizes the leaderboard of a Kaggle competition.",
  "icons": {
    "16": "icons/icon-16.png",
    "32": "icons/icon-32.png",
    "48": "icons/icon-64.png",
    "128": "icons/icon-128.png"
  },
  "content_scripts": [
    {
      "run_at": "document_idle",
      "js": ["scripts/leaderboard.js"],
      "matches": ["https://www.kaggle.com/competitions/*/leaderboard"]
    }
  ]
}
